{"ast":null,"code":"import{useState,useEffect}from\"react\";const useWindowSize=()=>{const[isMobile,setIsMobile]=useState(window.innerWidth<500);useEffect(()=>{const handleResize=()=>{setIsMobile(window.innerWidth<500);};window.addEventListener(\"resize\",handleResize);return()=>{window.removeEventListener(\"resize\",handleResize);};},[isMobile]);return isMobile;};export default useWindowSize;","map":{"version":3,"names":["useState","useEffect","useWindowSize","isMobile","setIsMobile","window","innerWidth","handleResize","addEventListener","removeEventListener"],"sources":["/home/shawen/Code/faq-accordion/accordion/src/hooks/ScreenSIze.tsx"],"sourcesContent":["import { useState, useEffect } from \"react\";\n\nconst useWindowSize = () => {\n  const [isMobile, setIsMobile] = useState(window.innerWidth < 500);\n\n  useEffect(() => {\n    const handleResize = () => {\n      setIsMobile(window.innerWidth < 500);\n    };\n\n    window.addEventListener(\"resize\", handleResize);\n\n    return () => {\n      window.removeEventListener(\"resize\", handleResize);\n    };\n  }, [isMobile]);\n\n  return isMobile;\n};\n\nexport default useWindowSize;\n"],"mappings":"AAAA,OAASA,QAAQ,CAAEC,SAAS,KAAQ,OAAO,CAE3C,KAAM,CAAAC,aAAa,CAAGA,CAAA,GAAM,CAC1B,KAAM,CAACC,QAAQ,CAAEC,WAAW,CAAC,CAAGJ,QAAQ,CAACK,MAAM,CAACC,UAAU,CAAG,GAAG,CAAC,CAEjEL,SAAS,CAAC,IAAM,CACd,KAAM,CAAAM,YAAY,CAAGA,CAAA,GAAM,CACzBH,WAAW,CAACC,MAAM,CAACC,UAAU,CAAG,GAAG,CAAC,CACtC,CAAC,CAEDD,MAAM,CAACG,gBAAgB,CAAC,QAAQ,CAAED,YAAY,CAAC,CAE/C,MAAO,IAAM,CACXF,MAAM,CAACI,mBAAmB,CAAC,QAAQ,CAAEF,YAAY,CAAC,CACpD,CAAC,CACH,CAAC,CAAE,CAACJ,QAAQ,CAAC,CAAC,CAEd,MAAO,CAAAA,QAAQ,CACjB,CAAC,CAED,cAAe,CAAAD,aAAa"},"metadata":{},"sourceType":"module","externalDependencies":[]}